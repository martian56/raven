// Minimal math library for testing
fun abs(x: float) -> float {
    if (x < 0.0) {
        return -x;
    } else {
        return x;
    }
}

fun max(a: float, b: float) -> float {
    if (a > b) {
        return a;
    } else {
        return b;
    }
}

fun min(a: float, b: float) -> float {
    if (a < b) {
        return a;
    } else {
        return b;
    }
}

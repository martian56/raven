{
  "name": "raven-language",
  "displayName": "Raven Language",
  "description": "Syntax highlighting and language support for Raven programming language",
  "version": "1.1.5",
  "publisher": "martian56",
  "icon": "raven.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "raven",
    "programming",
    "language",
    "syntax",
    "highlighting"
  ],
  "contributes": {
    "languages": [
      {
        "id": "raven",
        "aliases": [
          "Raven",
          "raven"
        ],
        "extensions": [
          ".rv"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "raven",
        "scopeName": "source.raven",
        "path": "./syntaxes/raven.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "raven",
        "path": "./snippets/raven.json"
      }
    ],
    "commands": [
      {
        "command": "raven.runFile",
        "title": "Run Raven File",
        "category": "Raven"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "raven.runFile",
          "when": "resourceExtname == .rv",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "raven.runFile",
          "when": "resourceExtname == .rv",
          "group": "navigation"
        }
      ]
    }
  },
  "activationEvents": [
    "onLanguage:raven"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/martian56/raven.git"
  },
  "homepage": "https://github.com/martian56/raven",
  "bugs": {
    "url": "https://github.com/martian56/raven/issues"
  },
  "license": "MIT"
}